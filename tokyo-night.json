{
  "name": "Tokyo Night",
  "style": "Dark",
  "isDark": true,
  "editor": {
    "backgroundColor": "#1a1b26",
    "altBackgroundColor": "#16161e",
    "tintColor": "#7aa2f7",
    "tintColor2": "#7dcfff",
    "textColor": "#c0caf5",
    "toolbarBackgroundColor": "#16161e",
    "toolbarIconColor": "#a9b1d6",
    "menuItemColor": "#a9b1d6",
    "timeBlockColor": "#7aa2f7",
    "shouldOverwriteFont": false,
    "sidebarStyleOverride": "dark",
    "sidebarIconColorOverride": "#7aa2f7"
  },
  
  "__orderedStyles": [
    "title-mark1",
    "title-mark2",
    "title-mark3",
    "body",
    "quote-content",
    "bold",
    "bold-left-mark",
    "bold-right-mark",
    "italic",
    "italic-left-mark",
    "italic-right-mark",
    "boldItalic",
    "boldItalic-left-mark",
    "boldItalic-right-mark",
    "code",
    "code-left-backtick",
    "code-right-backtick",
    "checked-todo-characters",
    "todo",
    "checked",
    "quote-mark",
    "tabbed",
    "link",
    "hashtag",
    "attag",
    "schedule-to-date-link",
    "done-date",
    "schedule-from-date-link",
    "title1",
    "title2",
    "title3"
  ],

  "styles": {
    "body": {
      "color": "#c0caf5",
      "font": "AvenirNext-Regular",
      "size": 16
    },
    "title1": {
      "font": "AvenirNext-Bold",
      "size": 28,
      "color": "#7aa2f7"
    },
    "title2": {
      "font": "AvenirNext-Bold",
      "size": 24,
      "color": "#7dcfff"
    },
    "title3": {
      "font": "AvenirNext-Bold",
      "size": 20,
      "color": "#bb9af7"
    },
    "title4": {
      "font": "AvenirNext-Bold",
      "size": 18,
      "color": "#b4f9f8"
    },
    "bold": {
      "font": "AvenirNext-Bold",
      "color": "#c0caf5"
    },
    "italic": {
      "font": "AvenirNext-Italic",
      "color": "#c0caf5"
    },
    "todo": {
      "color": "#c0caf5",
      "headIndent": 33,
      "size": 16,
      "lineSpacing": 0.90
    },
    "checked-todo-characters": {
      "headIndent": 33,
      "size": 16
    },
    "tabbed": {
      "headIndent": 43
    },
    "checked": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[x\\])([^@]*)",
      "color": "#565f89",
      "matchPosition": 0,
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "checked-canceled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\-\\] )(.*)",
      "matchPosition": 0,
      "color": "#66565f89",
      "strikethroughStyle": 1,
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "checked-scheduled": {
      "regex": "(^\\h*[\\*\\-\\+]{1} |^\\h*[0-9]+[\\.\\)] )(\\[\\>\\] )(.*)",
      "matchPosition": 0,
      "color": "#447dcfff",
      "headIndent": 33,
      "lineSpacing": 0.90
    },
    "quote-mark": {
      "color": "#565f89",
      "headIndent": 33
    },
    "quote-content": {
      "color": "#a9b1d6",
      "type": "italic",
      "headIndent": 33
    },
    "link": {
      "color": "#7aa2f7"
    },
    "schedule-to-date-link": {
      "color": "#7dcfff"
    },
    "done-date": {
      "color": "#565f89"
    },
    "schedule-from-date-link": {
      "color": "#7dcfff"
    },
    "hashtag": {
      "color": "#2ac3de",
      "backgroundColor": "#16161e",
      "borderRadius": 6
    },
    "attag": {
      "color": "#ff9e64",
      "backgroundColor": "#16161e",
      "borderRadius": 6
    },
    "highlighted": {
      "backgroundColor": "#292e42",
      "color": "#c0caf5"
    },
    "strikethrough": {
      "color": "#565f89",
      "strikethroughColor": "#565f89"
    },
    "underline": {
      "underlineStyle": 2,
      "underlineColor": "#ff9e64"
    },
    "code": {
      "font": "Menlo-Regular",
      "color": "#9ece6a",
      "backgroundColor": "#16161e",
      "size": 14
    },
    "code-left-backtick": {
      "color": "#66565f89"
    },
    "code-right-backtick": {
      "color": "#66565f89"
    },
    "code-fence": {
      "theme": "one-dark",
      "corner-radius": 8,
      "body": {
        "font": "Menlo-Regular",
        "backgroundColor": "#16161e",
        "size": 14
      },
      "language-specifier": {
        "font": "Menlo-Italic",
        "color": "#565f89",
        "size": 14
      },
      "fence-open": {
        "color": "#565f89"
      },
      "fence-close": {
        "color": "#565f89"
      }
    },
    "flagged-1": {
      "backgroundColor": "#db4b4b20",
      "color": "#db4b4b"
    },
    "flagged-2": {
      "backgroundColor": "#db4b4b30",
      "color": "#db4b4b"
    },
    "flagged-3": {
      "backgroundColor": "#db4b4b40",
      "color": "#db4b4b"
    },
    "working-on": {
      "backgroundColor": "#7aa2f720",
      "color": "#7aa2f7"
    },
    "comment": {
      "color": "#565f89",
      "italic": true
    },
    "special-char": {
      "color": "#db4b4b"
    },
    "pilcrow": {
      "regex": "(.*)Â¶(.*)",
      "matchPosition": 2,
      "color": "#db4b4b"
    },
    "field": {
      "color": "#9ece6a"
    },
    "attribute": {
      "color": "#2ac3de"
    },
    "template-tag": {
      "regex": "<%([^%]*)%>",
      "color": "#9ece6a",
      "backgroundColor": "#292e4240"
    },
    "template-comment": {
      "regex": "<%#([^%]*)%>",
      "color": "#565f89",
      "backgroundColor": "#292e4220"
    },
    "time-block-tag": {
      "regex": "(#ðŸ•‘)",
      "matchPosition": 0,
      "color": "#16161e",
      "backgroundColor": "#16161e"
    }
  }
}